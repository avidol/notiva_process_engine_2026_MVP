{
  "rulesetId": "MVP_RULESET",
  "version": "1.0",
  "rules": [
    {
      "ruleId": "REQ_POLICY",
      "type": "required",
      "field": "Content.policyNumber",
      "message": "Policy number is required"
    },
    {
      "ruleId": "LEN_POLICY",
      "type": "length",
      "field": "Content.policyNumber",
      "min": 5,
      "max": 20,
      "message": "Policy number length must be between 5 and 20"
    }
  ]
}
